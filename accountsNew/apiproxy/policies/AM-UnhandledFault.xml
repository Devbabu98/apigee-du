<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-UnhandledFault">
    <DisplayName>AM-UnhandledFault</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>api-error.status_code</Name>
        <Ref>response.status.code</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.reason_phrase</Name>
        <Ref>response.reason.phrase</Ref>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>