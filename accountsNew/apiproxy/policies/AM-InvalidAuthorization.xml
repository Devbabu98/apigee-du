<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-InvalidAuthorization">
    <DisplayName>AM-InvalidAuthorization</DisplayName>
    <FaultRules/>
    <AssignVariable>
        <Name>api-error.status_code</Name>
        <Value>401</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>api-error.reason_phrase</Name>
        <Value>invalid authorization</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false"/>
</AssignMessage>